{
  "name": "pumubot",
  "version": "1.0.0",
  "description": "Simple discord bot for managing interactions and notifications in my own server",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup src/index.ts --minify",
    "deploy-commands": "tsx ./src/runners/deploy-commands.runner.ts"
  },
  "imports": {
    "#core/*": "./src/core/*"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "Samuel Jurado",
  "license": "GPL-3.0",
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  }
}